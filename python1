x=int(input("Adj meg egy műveletet: "))
y=int(input("Adj meg egy számot: "))
z=int(input("adj meg egy számot: "))

def adas(y,z):
    return y+z
def kivon(y,z):
    return y-z
def szorzas(y,z):
    return(y*z)
if x==1:
    print(adas(y,z))
elif x==2:
    print(kivon(y,z))
elif x==3:
    print(szorzas(y,z))
else:
    print("rossz input!")






def maganh(x):
    maganhangzok=['a','á','o','e','ö','ü','ú','ű','í','i','é','ó','ő','u',]
    db=0
    for i in x:
        if i in maganhangzok:
            db+=1
    return db 
    
x=str(input("Adj meg egy szöbeget csak kis betűkkel :  "))    
darab=maganh(x)
print(darab)









def faktorialis(szam):
    fakt=1
    while(szam!=0):
        fakt=fakt*szam
        szam=szam-1
    return fakt
    
szam=int(input("Adj meg egy számot:  "))
print("Az adott szám faktoriálisa: %d" %faktorialis(szam))






def betuk(x):
    szam=[0,0]
    for i in x:
        if i.isupper():
            szam[0]+=1
        elif i.islower():
            szam[1]+=1
        else:
            pass
    return szam
    
x=input("Add mge a mondatot kis és nagybetű keverve :  ")
eredmeny=betuk(x)
print("A mondatban %d kis és %d nagybetű volt" %(eredmeny[0],eredmeny[1]))




def pascalh(n):
    sor=[1]
    y=[0]
    for x in range(n):
        print(sor)
        sor=[b+j for b,j in zip(sor+y, y+sor)]
    return n>=1
    
pascalh(10)


def legidosebb(fiu1,fiu2,fiu3):
    print("A legidösebb fiu "+fiu2)
    
legidosebb(fiu1="Erik",fiu2="Ádám",fiu3="Tamás")





def legfiatalabb(adatok):
    legfiatalabb=min(adatok[1])
    return adatok[1].index(legfiatalabb)

def legidosebb(adatok):
    legidosebb=max(adatok[1])
    return adatok[1].index(legidosebb)
    
adatok=[[],[]]
    
    

bemenet=input("Szeretnél adatokat madni? (i/n)")
while bemenet=='i':
    nev=input("Add meg a tljes neved :  ")
    adatok[0].append(nev)
    szulev=input("Add meg a zületési éved :  ")
    adatok[1].append(szulev)
    bemenet=input("Szeretnél adatokat madni? (i/n)")

    
    
legfiatalabb=adatok[0][legfiatalabb(adatok)]
legidosebb=adatok[0][legidosebb(adatok)]
    
    
print("A legfiatalabb :  %s" %legfiatalabb)
print("A legidosebb :  %s" %legidosebb)







def ellenoriz(telSzam):
    #elősször ellenőrizzük a sztring hosszát
    while not (len(telSzam)==12 or len(telSzam)==13): 
        print(len(telSzam))
        print("A szám hosszúsága nem megfelelő!")
        telSzam=input("""Adj meg egy formailag helyes mobilszámot!
    A formailag elfogadható számok: +36XXYYYYYYY vagy 0036XXYYYYYYY""")    
    #ellenőrizzük hogy megfelelően kezdődjön
    while not (telSzam[0:3]=='+36' or telSzam[0:4]=='0036'):
        print("A szám '+36'-tal vagy '0036'-tal kell, hogy kezdődjön")
        telSzam=input("""Adj meg egy formailag helyes mobilszámot!
A formailag elfogadható számok: +36XXYYYYYYY vagy 0036XXYYYYYYY""")
    
    #végül ellenőrizzük, hogy minden számjegy szám legyen
    for i in range(-1,-11,-1):
        if telSzam[i].isdecimal():
            seged=True
        else:
            print("Nem megfelelő szám!")
            seged=False
            break
    #ha valamelyik karakter nem szám, akkor újra kérjük
    while not seged:
        telSzam=input("""Adj meg egy formailag helyes mobilszámot!
A formailag elfogadható számok: +36XXYYYYYYY vagy 0036XXYYYYYYY""")
    #végül vissza adjuk az ellenőrzött sztringet
    return telSzam

#itt kezdődik maga a program


telenor=(0)
telekom=(0)
digi=(0)
vodafone=(0)
kekszam=(0)
zoldszam=(0)

for s in range(5):
    telSzam=input("""Adj meg még egy formailag helyes mobilszámot!(összesen 5 Telefonszámot adhatsz meg!)
A formailag elfogadható számok: +36XXYYYYYYY vagy 0036XXYYYYYYY : """)
#függvényhívással ellenőrizzük
    telSzam=ellenoriz(telSzam)
#végül kategorizáljuk szolgáltatók szerint
    if telSzam[-9:-7]=='20':
        print("Az adott szám a Telenor szolgáltatóhoz tartozik!")
        telenor+=1
    elif telSzam[-9:-7]=='30':
        print("Az adott szám a Telekom szolgáltatóhoz tartozik!")
        telekom+=1
    elif telSzam[-9:-7]=='50':
        print("Az adott szám a Digi szolgáltatóhoz tartozik!")
        digi+=1
    elif telSzam[-9:-7]=='70':
        print("Az adott szám a Vodafone szolgáltatóhoz tartozik!")
        vodafone+=1
    elif telSzam[-9:-7]=='40':
        print("Az adott szám egy kékszám!")
        kekszam+=1
    elif telSzam[-9:-7]=='80':
        print("Az adott szám egy zöldszám")
        zoldszam+=1

print("Telenorból ennyi van: %d" %telenor)
print("Telekomból ennyi van: %d" %telekom)
print("digiből ennyi van: %d" %digi)
print("vodafone-ból ennyi van: %d" %vodafone)
print("kékszámból ennyi van: %d" %kekszam)
print("zöldszámból ennyi van %d" %zoldszam)










jegyek=[]

def osztalyzat(pont):
    pont=int(pont)
    if (pont >=0 and pont <=15):
        jegyek.append(1)
        print("Ez a pont elégtelen (1)")
    elif (pont >=16 and pont <=25):
        jegyek.append(2)
        print("Ez a pont elégséges (2)")
    elif (pont >=26 and pont <=35):
        jegyek.append(3)
        print("Ez a pont közepes (3)")
    elif (pont >=36 and pont <=45):
        jegyek.append(4)
        print("Ez a pont Jó (4)")
    elif (pont >=46 and pont <=50):
        jegyek.append(5)
        print("Ez a pont jeles (5)")
    else: 
        pass
    
def atlag(jegyek):
    osszeg=0
    for i in range(len(jegyek)):
        osszeg=osszeg+jegyek[i]
    atlag=float(osszeg/len(jegyek))
    return atlag

def jegydarab(jegyek):
    darab=[]
    for i in range(1,6):
        darab.append(jegyek.count(i))
    return darab

def parossag(jegyek):
    parossag=[0,0]
    for i in range(len(jegyek)):
        if jegyek[i]%2:
            parossag[0]+=1
        elif not jegyek[i]%2:
            parossag[1]+=1
        else:
            pass
    return parossag

def bukás(jegyek):
    elegtelen=jegyek.count(1)
    bukas=float((elegtelen/len(jegyek))*100)
    print("Az osztály bukási aránya : %f"%bukas)
    
    
for i in range(20):
    pont=input("Add meg a diák pontszámát: ")
    osztalyzat(pont)
atlag=atlag(jegyek)
print("Az osztály átlaga : %f"%atlag)
darab=jegydarab(jegyek)
print("Az egyes osztályzatok darabszáma rendre: ")
print(darab)
parossag=parossag(jegyek)
print("páratln osztályzatok száma : %d  páros osztályzatok száma : %d "%(parossag[0],parossag[1]))
bukás(jegyek)




def szamell(x):
    while (not (len(x)==6 and x.isdigit())):
        print("A szán nem megfelelő hosszuságu")
        return False
    return True

def helyes(x):
    osszeg=0
    for i in range(len(x)-1):
        osszeg=osszeg+int(x[i])
    maradek=osszeg%6
    while maradek!=int(x[-1]):
        print("A kód nem felel meg a kritériumoknak")
        return False
    return True

kodok=[]

x=input("Add meg a termék kódját")
while x!='q':
    while (not (szamell(x) and helyes(x))):
        x=input("add meg újra helyesen")
    kodok.append(x)
    
    

    
print(kodok)
print("kilép")





def szmhosz(x):
    while (len(x)>20):
        print("Nem megfelelő hosszuságu szám")
        x=str(input("Addj meg egy max 20 betűből állo mondatot"))

def maganh(x):
    maganhangzok=['a','á','o','e','ö','ü','ú','ű','í','i','é','ó','ő','u',]
    db=0
    for i in x:
        if i in maganhangzok:
            db+=1
    return db 

x=str(input("Adj meg egy max 20 karakterből állo szöveget"))
lista=[]




szmhosz(x)
maganh(x)
for i in x:
    lista.append(i)

print(lista)
darab=maganh(x)
print(darab)
