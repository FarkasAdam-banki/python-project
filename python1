tomb=[]
tomb.append(int(input("Adj meg egy zámot")))
for i in range(1,10):
    szam=(int(input("Adj meg egy számot:  ")))
    while szam in tomb:
        szam=(int(input("Ez a szám már létezik adj meg eg ujat:  ")))



for i in range(len(tomb)-1):
    for j in range(i+1,len(tomb)):
        if tomb[j]>tomb[j+1]:
            temp=tomb[j+1]
            tomb[j+1]=tomb[j]
            tomb[j]=temp
print(tomb)

#beszurásos:
for i in range(1,len(tomb):
    kulcs=tomb[i]
    j=i-1
    while j>=0 and tomb[j]>kulcs:
        tomb[j+1]=kulcs
               
print(tomb)



import random
t=[random.randint(1,1000)]
for i in range(1,99):
    t.append(random.randint(1,1000))



fele=int(len(t)/2)+1
print(fele)
for i in range(fele-1):
    for j in range(i+1,fele):
        if t[i]>t[j]:
            temp=t[j]
            t[j]=t[i]
            t[i]=temp
for i in range(fele+1,len(t)-1):
    for j in range(i+1,len(t)):
        if t[i]<t[j]:
            temp=t[j]
            t[j]=t[i]
            t[i]=temp
t[fele]=0
print(t)





