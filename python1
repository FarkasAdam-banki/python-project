a=int(input("add meg az első számot:"))
b=int(input("ad meg a második számot:"))

while a<0:
    print("a szám csak pozitiv lehet!")
    a=(input("add meg az első számot:"))
    
while b<0:
    print("a szám csak pozitiv lehet!")
    b=(input("add meg az első számot:"))
    

    if a<b:
        seged=a
        a=b
        b=seged
    
    maradek=a % b
    
    while maradek>0:
        a=b
        b=maradek
        maradek= a % b
        
    lko=b
    print("a legnagyobb közös osztó a %d" % lko)


n=10
for i in range(1,n):
    for j in range(n):
        if j==i or j==n-i:
            print("*",end="")
        else:
            print(" ", end="")
    print("")
    
    
num = int(input("ird be hány sor legyen:"))
row = 0
while row<num:
    space = num-row-1
    while space>0 :
        print(end="")
        space = space-1
    star = row+1
    while star>0:
        print("*", end=" ")--3,3
        star = star-1
    row = row+1
    print()



