szam=int(input("Adj meg egy számot"))
while szam==0:
    szam=int(input("adj meg egy számot"))
szamok=[szam]
while szam !=0:
    szam=int(input("Adj meg egy számot"))
    szamok.append(szam)
szamok.pop()
print(szamok)
minimum=szamok[0]
for i in range(1,len(szamok)):
    if minimum>szamok[i]:
        minimum=szamok[i]
print(minimum)


maximum=szamok[0]
for i in range(1,len(szamok)):
    if maximum<szamok[i]:
        maximum=szamok[i]
print(maximum)





