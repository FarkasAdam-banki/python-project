print("\nrange(6):  \t", end="")
#0 1 2 3 4 5
for x in range(6):
    print("%4d" %x, end="")
    
print("\nrange(3,9): \t", end="")
#3 4 5 6 7 8
for x in range(3, 9):
    print("%4d" % x, end="")
    
print("\nrange (3,14,2): \t", end="")
#3 5 7 9 11 13
for x in range(3, 14, 2):
    print("%4d" % x, end="")

print("\nrange(14,3,-2): \t", end="")
#14 12 10 8 6 4
for x in range (14,3,-2):
    print("%4d" % x, end="")
print("\n")

